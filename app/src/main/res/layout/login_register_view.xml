<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" xmlns:tools="http://schemas.android.com/tools"
				android:id="@+id/login_register_view">
	<TextView
			android:id="@+id/login_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignStart="@+id/login_register_view"
			android:layout_alignLeft="@+id/login_register_view"
			android:layout_alignParentTop="true"
			android:layout_marginStart="90dp"
			android:layout_marginLeft="90dp"
			android:layout_marginTop="30dp"
			android:text="@string/login"
			android:textColor="#000"
			android:textSize="16sp"
			tools:ignore="NotSibling" />

	<TextView
			android:id="@+id/register_text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_alignParentRight="true"
			android:layout_marginTop="30dp"
			android:layout_marginEnd="90dp"
			android:layout_marginRight="90dp"
			android:text="@string/register"
			android:textColor="#000"
			android:textSize="16sp"
			tools:ignore="NotSibling" />

	<com.example.loopdaily.lib.ViewPagerUtil android:layout_width="wrap_content"
											 android:id="@+id/login_register_viewpager"
											 android:layout_below="@+id/login_text"
											 android:layout_marginTop="0dp"
											 android:layout_height="wrap_content"/>
</RelativeLayout>